extends layout

block content
    .container
        // 로그 그래프
        .row
            .col-xs-6.col-xs-offset-3
                .ct-chart.ct-perfect-fourth
                    script.
                        graphFrequencies(!{JSON.stringify(logdataGraph)})

        // 괸심 뉴스들
        block userLikes
            if articleList
                +articleList_Tiled(articleList.title, articleList.articles)

        // 로그 리스트
        .row
            table.table.table-bordered.table-striped.table-condensed
                thead
                    tr
                        th= '시간'
                        th= '제목'
                        th= '카테고리'
                        th= '클러스터'
                tbody
                    each log in logs
                        tr
                            td= formatDate(log.startedAt)
                            td
                                a(href='/articles/#{encodeURIComponent(log.article._id)}')=log.article.title
                            td= log.article.cate
                            td= log.article.cluster
